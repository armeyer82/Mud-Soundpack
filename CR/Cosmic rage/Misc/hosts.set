#TRIGGER {[[]administrators]*} {playsound {Cosmic Rage\hosts\AdminTalk.wav} @vol; #additem Admin %0; #if {@ChannelLogging = 1} {#file 1 {cosmic rage/logs/%char admin.log};#write 1 %0;#close 1}} {} AnyCase
