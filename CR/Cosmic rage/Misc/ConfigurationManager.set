#math musicVol {@musicVol + 5};
#if {@musicVol > 100} {@MusicVol = 100};
playsound {Cosmic Rage/general/misc/VolumeUp.wav} @musicVol; #say {Planet music Volume @musicVol percent};
#if {@musicplaying = 1} {
#pc @musicHandle volume @musicvol}}
