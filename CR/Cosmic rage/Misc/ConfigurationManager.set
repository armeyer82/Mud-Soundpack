;Volumes
#alias  genvoldown {
#math Vol {@Vol - 5};
#if {@Vol < 0} {@Vol = 0};
playsound {Cosmic Rage/general/misc/VolumeDown.wav} @vol; #say {Main Volume @Vol percent}}
#alias genvolup {
#math Vol {@Vol + 5};
#if {@Vol > 100} {@Vol = 100};
playsound {Cosmic Rage/general/misc/VolumeUp.wav} @vol; #say {Main Volume @Vol percent}}; #if {@vol = 100} {
#say {Volume maxed!}}

#alias  gvoldown {
#math gvol {@gvol - 5};
#if {@gvol < 0} {@gvol = 0};
playsound {Cosmic Rage/general/misc/VolumeDown.wav} @gvol; #say {Ground Combat Volume @gvol percent}}
#alias gvolup {
#math gvol {@gvol + 5};
#if {@gvol > 100} {@gvol = 100};
playsound {Cosmic Rage/general/misc/VolumeUp.wav} @gvol; #say {Ground Combat Volume @gvol percent}}; #if {@gvol = 100} {
#say {Volume maxed!}}

#alias {CommsVolUp} {
#math CommsVol {@CommsVol + 5};
#if {@CommsVol > 100} {@CommsVol = 100};
playsound {Cosmic Rage/general/misc/VolumeUp.wav} @CommsVol; #say {Communication Volume @CommsVol percent}};
#if {@CommsVol = 100} {
#say {Communication Volume maxed!}}

#alias  CommsVolDown {
#math CommsVol {@CommsVol - 5};
#if {@CommsVol < 0} {@CommsVol = 0};
playsound {Cosmic Rage/general/misc/VolumeDown.wav} @CommsVol; #say {Communication Volume @CommsVol percent}}

