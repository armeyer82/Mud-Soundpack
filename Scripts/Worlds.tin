#CLASS {Worlds} OPEN

#ALIAS {add-world}
{
	#showme Adding %1;
	#variable World[%1][Host] %2;
	#variable World[%1][Port] %3
}
{5}

#ALIAS {connect}
{
	#session %1 $World[%1][Host] $World[%1][Port]
}
{5}

#ALIAS {delete-world}
{
	#unvariable World[%1];	#show Deleted %1
}
{5}

#VARIABLE         {World}  {{Miriani}{{Host}{moo.toastsoft.net}{Port}{1234}}}

#CLASS {Worlds} CLOSE
